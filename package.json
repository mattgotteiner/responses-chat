{
  "name": "responses-chat",
  "version": "1.0.0",
  "description": "A React + TypeScript chat interface for Azure OpenAI's Responses API",
  "author": "Matthew Gotteiner",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattgotteiner/responses-chat.git"
  },
  "homepage": "https://github.com/mattgotteiner/responses-chat",
  "keywords": [
    "react",
    "typescript",
    "azure",
    "openai",
    "chat",
    "responses-api"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:tunnel": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "generate-icons": "node scripts/generate-icons.mjs"
  },
  "dependencies": {
    "openai": "^6.17.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.1.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "canvas": "^3.2.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^17.2.0",
    "jsdom": "^27.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
